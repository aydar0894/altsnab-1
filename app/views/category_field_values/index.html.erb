<p id="notice"><%= notice %></p>

<h1>Category Field Values</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Category field</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @category_field_values.each do |category_field_value| %>
      <tr>
        <td><%= category_field_value.item %></td>
        <td><%= category_field_value.category_field %></td>
        <td><%= category_field_value.value %></td>
        <td><%= link_to 'Show', category_field_value %></td>
        <td><%= link_to 'Edit', edit_category_field_value_path(category_field_value) %></td>
        <td><%= link_to 'Destroy', category_field_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Category Field Value', new_category_field_value_path %>
